<div class="content">
  <div class="page-title"><h1>Listing Aws Data</h1></div>
  <p id="notice"><%= notice %></p>

  <table>
    <thead>
    <tr>
      <th>Aws access key</th>
      <th>Aws secret key</th>
      <th>Manifest template</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @aws_data.each do |aws_datum| %>
      <tr>
        <td><%= aws_datum.aws_access_key %></td>
        <td><%= aws_datum.aws_secret_key %></td>
        <td><%= aws_datum.manifest_template %></td>
        <td><%= link_to 'Show', aws_datum %></td>
        <td><%= link_to 'Edit', edit_aws_datum_path(aws_datum) %></td>
        <td><%= link_to 'Destroy', aws_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Aws datum', new_aws_datum_path %>

</div>
